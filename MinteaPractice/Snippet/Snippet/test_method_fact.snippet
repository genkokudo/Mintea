<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
      <Title>test_method_fact</Title>
      <Author>Y.Toyoda</Author>
      <Description>単純なテストメソッドを作成します。</Description>
      <HelpUrl>www.microsoft.com</HelpUrl>
      <Shortcut>test_method_fact</Shortcut>
    </Header>
    <Snippet>
      <Imports>
        <Import>
          <Namespace>Xunit</Namespace>
        </Import>
        <Import>
          <Namespace>ChainingAssertion</Namespace>
        </Import>
        <Import>
          <Namespace>Xunit.Abstractions</Namespace>
        </Import>
      </Imports>
      <Code Language="CSharp" Kind="method decl" Delimiter="$"><![CDATA[
        [Fact(DisplayName = "テスト名称")]
        [Trait("Category", "分類名")]
        [Trait("Priority", "2")]        // TODO:小さい方が優先
        public void Test1()
        {
            string testData = "test";

            var result = TestClass.TestMethod(testData);
            output.WriteLine(result);

            result.Is("期待結果");
        }]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>